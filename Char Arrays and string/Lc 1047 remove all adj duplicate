// lc 1047
class Solution {
public:
    string removeDuplicates(string s) {
          string ans = "";
      int index =0;
      while(index <  s.length()){
        //same wala kase 
        //ans ka right most  char and string s ka current character
         if(  ans.length()> 0 && ans[ans.length()-1]== s[index]){
             // pop karo ans see
             
             ans.pop_back();
         }
         else {
            // push
            ans.push_back(s[index]);
         }
         index ++;

      }
      return ans;


    }
};
